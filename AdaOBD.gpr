project AdaOBD is

   for Source_Dirs use ("src/**");
   for Object_Dir use "obj";
   for Main use ("main.adb");
   for Target use "arm-linux-gnueabihf";

   package Gnattest is
      for Tests_Root use "../test/";
   end Gnattest;

   package Compiler is
      for Switches ("ada") use ("-gnat2020");
   end Compiler;

end AdaOBD;

